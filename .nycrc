{
  "require": [
    "babel-register"
  ],
  "sourceMap": false,
  "instrument": false,
  "cache": true,
  "reporter": [
    "html",
    "text",
    "lcov"
  ],
  "watermarks": {
    "lines": [
      80,
      95
    ],
    "functions": [
      80,
      95
    ],
    "branches": [
      80,
      95
    ],
    "statements": [
      80,
      95
    ]
  },
  "report-dir": "istanbul"
}

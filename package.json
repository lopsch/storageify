{
  "name": "storageify",
  "version": "0.0.0-development",
  "author": "Daniel Lopes <lopsch@googlemail.com>",
  "description": "Store/read Cycle.js state (from Cycle.js onionify) in/from HTML5 Local Storage",
  "main": "lib/index.js",
  "module": "lib/es6/index.js",
  "browser": "lib/umd/index.js",
  "min": "lib/umd/index.min.js",
  "unpkg": "lib/umd/index.js",
  "license": "MIT",
  "keywords": [
    "cyclejs",
    "onionify",
    "local storage"
  ],
  "files": [
    "lib",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/lopsch/storageify/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lopsch/storageify"
  },
  "devDependencies": {
    "@cycle/run": "3.2.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-minify": "0.2.0",
    "babel-register": "6.26.0",
    "benchmark": "2.1.4",
    "chai": "4.1.1",
    "chai-asserttype": "1.0.5",
    "chalk": "2.1.0",
    "codecov": "2.3.0",
    "commitizen": "2.9.6",
    "coveralls": "2.13.1",
    "cross-spawn": "5.1.0",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "4.5.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "fs-extra": "4.0.1",
    "jsdoc": "3.5.4",
    "mocha": "3.5.0",
    "mochawesome": "2.3.0",
    "npm-run-all": "4.1.1",
    "nyc": "11.1.0",
    "prettier": "1.6.0",
    "rimraf": "2.6.1",
    "rollup": "0.49.2",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-babel-minify": "3.1.2",
    "rollup-plugin-commonjs": "8.2.0",
    "rollup-plugin-eslint": "4.0.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-progress": "0.4.0",
    "rollup-watch": "4.3.1",
    "semantic-release": "7.0.2"
  },
  "dependencies": {
    "xstream": "10.9.0"
  },
  "scripts": {
    "test:dev": "mocha --colors --require babel-register --watch test/**/*.js",
    "test:cover": "nyc mocha --colors --require babel-register --reporter mochawesome --reporter-options reportFilename=index,reportDir=mochawesome,quiet=false,enableCode=false test/**/*.js",
    "test": "mocha --colors --require babel-register test/**/*.js",
    "cover": "rimraf istanbul mochawesome && npm run build:cover && npm run test:cover && npm run cover:check",
    "cover:report": "nyc report --reporter=html --reporter=text-lcov > istanbul/coverage.lcov",
    "cover:check": "nyc check-coverage --lines 0 --functions 0 --branches 0 --statements 0",
    "report:codecov": "codecov --disable=gcov",
    "report:coveralls": "cat istanbul/coverage.lcov | coveralls",
    "doc": "rimraf jsdoc && jsdoc -d jsdoc -r src",
    "build:perf": "rimraf lib && export NODE_ENV=performance && rollup -c rollup.config.dev.js",
    "build:cover": "rimraf lib && export NODE_ENV=development && export BABEL_ENV=coverage && rollup -c rollup.config.dev.js && unset BABEL_ENV",
    "build:dev": "rimraf lib && export NODE_ENV=development && rollup -c rollup.config.dev.js -w",
    "build:ci": "npm run clean && npm run cover && npm run build && npm run doc",
    "build": "rimraf lib && export NODE_ENV=production && export BABEL_ENV=production && rollup -c rollup.config.prod.js",
    "fix": "prettier --write --print-width 80 --tab-width 2 --no-semi --single-quote --trailing-comma none --jsx-bracket-same-line --parser babylon src/**/*.js test/**/*.js && eslint --fix src/**/*.js test/**/*.js",
    "perf": "npm run build:perf && babel-node perf/index.perf.js",
    "clean": "rimraf istanbul jsdoc lib mochawesome",
    "release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

{
  "name": "storageify",
  "version": "0.1.4",
  "author": "Daniel Lopes <lopsch@googlemail.com>",
  "description": "Store/read Cycle.js state (from Cycle.js onionify) in/from HTML5 Local Storage",
  "main": "lib/index.js",
  "license": "MIT",
  "keywords": [
    "cyclejs",
    "onionify",
    "local storage"
  ],
  "bugs": "https://github.com/lopsch/storageify/issues",
  "repository": "https://github.com/lopsch/storageify",
  "devDependencies": {
    "@cycle/run": "3.1.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-babili": "0.1.4",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.24.1",
    "benchmark": "2.1.4",
    "chai": "4.1.1",
    "chai-asserttype": "1.0.5",
    "chalk": "2.1.0",
    "cross-spawn": "5.1.0",
    "eslint": "4.4.1",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "fs-extra": "4.0.1",
    "jsdoc": "3.5.4",
    "mocha": "3.5.0",
    "mochawesome": "2.3.0",
    "nyc": "11.1.0",
    "prettier": "1.5.3"
  },
  "dependencies": {
    "xstream": "10.9.0"
  },
  "scripts": {
    "test-dev-min": "export NODE_ENV=test && export REPORTER=min && babel-node scripts/test/test.dev.js",
    "test-dev": "export NODE_ENV=test && babel-node scripts/test/test.dev.js",
    "test-ci": "export NODE_ENV=test && export BABEL_ENV=ci && babel-node scripts/test/test.ci.js",
    "coverage": "babel-node scripts/coverage/coverage.js",
    "doc": "babel-node scripts/doc/doc.js",
    "dist-dev": "export NODE_ENV=test && babel-node scripts/build/build.dev.js",
    "dist-ci": "export NODE_ENV=production && babel-node scripts/build/build.ci.js",
    "dist-ci-min": "export NODE_ENV=production && export BABEL_ENV=min && babel-node scripts/build/build.ci.js",
    "lint-dist": "npm run lint && npm run dist-ci",
    "ci": "npm run lint && npm run test-ci && npm run coverage && npm run dist-ci-min && npm run doc && npm run lib",
    "lib": "babel-node scripts/build/lib.js",
    "lint": "babel-node scripts/linting/linting.js",
    "prettier": "babel-node scripts/prettier/prettier.js",
    "format": "babel-node scripts/prettier/prettier.format.js && babel-node scripts/linting/linting.fix.js",
    "perf": "babel-node perf/index.perf.js"
  }
}
